{
  "name": "Agent Legacy Protected App",
  "baseURI": "https://miamitest.frdpcloud.com",
  "condition": "${matches(request.uri.path, '^(/charlieagentapp)')}",
  "capture": [
    "response",
    "request"
  ],
  "handler": "Chain",
  "heap": [
    {
      "name": "Chain",
      "type": "Chain",
      "config": {
        "filters": [
          "ChangeLocationFilter-Response"
        ],
        "handler": "ReverseProxyHandler"
      }
    },
    {
      "name": "ChangeLocationFilter-Response",
      "type": "LocationHeaderFilter",
      "config": {
        "baseURI": "https://ig.dev.miami-accelerators.com"
      }
    },
    {
      "name": "ReverseProxyHandler",
      "type": "ReverseProxyHandler",
      "config": {
        "connectionTimeout": "60 seconds",
        "soTimeout": "unlimited"
      },
      "capture": [
        "response",
        "request"
      ]
    }
  ]
}
{
  "name": "J2EE Legacy Protected App",
  "baseURI": "https://miamitest.frdpcloud.com",
  "condition": "${matches(request.uri.path, '^/apps/j2ee')}",
  "capture": [
    "response",
    "request"
  ],
  "heap": [
    {
      "name": "ReverseProxyHandler",
      "type": "ReverseProxyHandler",
      "capture": [
        "response",
        "request"
      ]
    },
    {
      "name": "capture",
      "type": "CaptureDecorator",
      "config": {
        "captureEntity": true,
        "captureContext": true
      }
    },
    {
      "name": "ForgeRockClientHandler",
      "type": "Chain",
      "config": {
        "filters": [
          "TransactionIdOutboundFilter"
        ],
        "handler": "ClientHandler"
      },
      "capture": [
        "response",
        "request"
      ]
    }
  ],
  "handler": "ReverseProxyHandler"
}